{
  "compileOnSave": true,
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "target": "ES2017",
    "module": "commonjs",
    "lib": [
      "es2017"
    ],
    "sourceMap": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@rest-stager-config": [ "src/lib/config/config" ],
      "@rest-stager-interfaces/*": ["src/lib/interfaces/*"]
    },
    "typeRoots": [
      "node_modules/@types"
    ]
  }
}